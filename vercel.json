{
  "version": 2,
  "builds": [
    {
      "src": "server.js",  
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/requerimiento",
      "dest": "/server.js"
    },
    {
      "src": "/api/requerimiento/decidir",
      "dest": "/server.js"
    },
    {
      "src": "/api/requerimientos",
      "dest": "/server.js"
    },
    {
      "src": "/api/decidir/(.*)",
      "dest": "/server.js"
    }
  ]
}
